    <style>
        .cardWeather {
            display: flex;
            justify-content: center;
            align-items: center;
            /*height: 100vh;*/
            height: 600px;
            max-width: 100%;
            margin: 0;
            font-family: 'Open Sans', sans-serif;
            background: #222;
